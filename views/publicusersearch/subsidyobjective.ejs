<script>

$(document).ready(function () {

  if (($('#subsidyobjective-7').is(":checked")) && ($('#subsidyobjective-0').is(":checked"))) {
    $('.govuk-input').hide();
    $('.govuk-label-other').hide(); 
  }

  else if ($('#subsidyobjective-11').is(":checked")){
    $('.govuk-input').show();
    $('.govuk-label-other').show(); 
  }

else {
  $('.govuk-input').hide();
  $('.govuk-label-other').hide();

}

$("#subsidyobjective-0").change(function(){  //"select all" change 
    $(".govuk-checkboxes__input").prop('checked', $(this).prop("checked")); //change all ".checkbox" checked status
    if($('#subsidyobjective-0').is(":checked")){
     
    $('.govuk-input').hide();
    $('.govuk-label-other').hide(); }

});//"select all" close

//".checkbox" change 

$('#subsidyobjective-11').change(function(){ 

if($('#subsidyobjective-0').is(":checked")){
$('.govuk-input').hide();
$('.govuk-label-other').hide();
}

else if ($('#subsidyobjective-11').is(":checked")){
 
  $('.govuk-input').show();
$('.govuk-label-other').show();}

// else if ($('#subsidyobjective-11').not(":checked")){
//   $('.govuk-input').show();
// $('.govuk-label-other').show();}

else {
  $('.govuk-input').hide();
$('.govuk-label-other').hide();

}

});


$('.govuk-checkboxes__input').change(function(){ 
	  //uncheck "select all", if one of the listed checkbox item is unchecked
    if(false == $(this).prop("checked")){ //if this item is unchecked
        $("#subsidyobjective-0").prop('checked', false); //change "select all" checked status to false
    // alert("change fn" + $('#subsidyobjective-11').is(":checked"))
   if($('#subsidyobjective-11').is(":checked")) {

    $('.govuk-input').show();
    $('.govuk-label-other').show(); }
    }

  // alert($('.govuk-checkboxes__input:checked').length );
	//check "select all" if all checkbox items are checked
	if ($('.govuk-checkboxes__input:checked').length == $('.govuk-checkboxes__input').length - 1 ){
    $("#subsidyobjective-0").prop('checked', true); 
    $('.govuk-input').hide();
    $('.govuk-label-other').hide(); 
  }
  

  
    });

  });


</script> 

